{
  "name": "MykeTheme",
  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "background": "#000000",
        "foreground": "#ffffffaa"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#6dda67"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#DC7D58"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#e5949b"
      }
    },
    {
      "name": "Identifier",
      "scope":
        "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#108FEF"
      }
    },

    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#C17656"
      }
    },
    {
      "name": "Keyword",
      "scope": "modifier",
      "settings": {
        "foreground": "#2566AA"
      }
    },
    {
      "name": "Keyword",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#b374fa"
      }
    },
    {
      "name": "Keyword",
      "scope": "support.type.object",
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "name": "Keyword",
      "scope": "constant.language",
      "settings": {
        "foreground": "#00ff00ffccff"
      }
    },

    {
      "name": "Function call",
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#77cadd"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type",
      "settings": {
        "foreground": "#DE627D"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#dcc258"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#108FEF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "support.type",
      "settings": {
        "foreground": "#884D94"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#f4a71a"
      }
    },
    {
      "name": "Type",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#f4a71a"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#7C3A84",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#f4f900",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property",
      "settings": {
        "foreground": "#21C5C7"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#21C5C7"
      }
    },
    {
      "name": "Function definition",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#21C5C7"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#00ccff"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#108FEF"
      }
    },
    {
      "name": "Modifier",
      "scope": "modifier",
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "name": "JSON key",
      "scope":
        "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#108FEF"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#00ccff"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#f2c460"
      }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#f2ab60"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#63d84b"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#979797"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#cc6e51"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#dfbc70"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#00ccff"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#108FEF"
      }
    },
    {
      "name": "Markdown block quote",
      "scope": "text.html.markdown markup.quote",
      "settings": {
        "foreground": "#c0c0c0"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#00ccff"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#108FEF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#108FEF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#108FEF",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#108FEF"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#00ccff"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#21C5C7",
        "fontStyle": "underline"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#21C5C7"
      }
    },
    {
      "name": "C# function call",
      "scope":
        "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#fff099"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#4EB071"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#4EB071"
      }
    },
    {
      "name": "C# preprocessor",
      "scope": "source.cs meta.preprocessor",
      "settings": {
        "foreground": "#DA6771"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#ffffffaa"
      }
    },
    {
      "name": "Global settings",
      "settings": {
        "background": "#000000",
        "foreground": "#ffffffaa"
      }
    }
  ],
  "colors": {
    "HERE-DUDE":
      "https://code.visualstudio.com/docs/getstarted/theme-color-reference",
    "editorBracketMatch.background": "#c9891c",
    "focusBorder": "#DC7D58",
